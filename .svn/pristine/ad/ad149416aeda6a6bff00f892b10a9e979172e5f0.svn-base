a:link, a:visited {
    display: block;
    color: #000000;
    width: 120px;
    text-align: center;
    padding: 4px;
    text-decoration: none;
    border-style:solid;
    border-width:2px;
    border-color:#F5FFF0;
    border-collapse:collapse;
}
a:hover, a:active {
    background-color: #F5D6F0;
}
